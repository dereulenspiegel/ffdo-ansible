log syslog all;
router id {{bird6_router_id}};

{% for template in bird6_templates %}
template {{template.header}} {
  {{template.content|indent(2,false)}}
};

{% endfor %}
{% for protocol in bird6_protocols %}
protocol {{protocol.header}} {
  {{protocol.content|indent(2,false)}}
};

{% endfor %}