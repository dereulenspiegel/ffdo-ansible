log syslog all;
router id {{routerid}};

protocol direct {
        interface "*";
};

protocol kernel {
        device routes;
        import all;
        export all;
        kernel table 42;
};

protocol device {
        scan time 8;
};

include "{{bird_config_dir}}/conf.d/*.conf";
