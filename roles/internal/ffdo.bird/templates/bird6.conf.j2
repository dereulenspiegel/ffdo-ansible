log syslog all;
router id {{routerid}};

protocol static uplink_hostroute {
   import all;
   export all;
}

protocol direct {
        interface "*";
}

protocol kernel {
        persist;
        device routes;
        scan time 20;
        import all;
        export all;
        kernel table 42;
}

protocol device {
        scan time 10;
}

include "{{bird_config_dir}}/conf6.d/*.conf";
