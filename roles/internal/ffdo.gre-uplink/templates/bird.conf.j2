{% if supernode_uplinks is defined %}
{% for uplink in supernode_uplinks %}
protocol bgp {{uplink.name}} from ibgp {
        source address {{uplink.address}};
        neighbor {{uplink.dstaddr}} as {{uplink.as}};
        default bgp_med 2;
};

{% endfor %}
{% endif %}