{% for meshlink in supernode_mesh_backbones%}
protocol bgp {{meshlink.name}} from ibgp {
        source address {{supernode_mesh_ipv4|ipaddr('address')}};
        neighbor {{meshlink.mesh_ip}} as {{supernode_as}};
        default bgp_med 4;
};
{% endfor %}