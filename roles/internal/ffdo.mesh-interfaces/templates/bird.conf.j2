{% for host in hostvars %}
{% if host['ansible_fqdn'] != ansible_fqdn %}
protocol bgp {{host['ansible_hostname']}} from ibgp {
        source address {{mesh_ipv4|ipaddr('address')}};
        neighbor {{host.mesh_ipv4}} as {{supernode_as}};
        default bgp_med 4;
};
{% endif %}
{% endfor %}